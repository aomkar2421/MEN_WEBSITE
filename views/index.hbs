    <section id="services">
      <div class="container">
        <div class="text-secondary">
          Services That We Offer
        </div>

        <div class="row">

          {{#each service}}
          <div class="col-md-4">

            <div class="card mt-6">

              <div class="card-body">
                <div class="container text-center">
                  <i class=" {{this.icon}} service-txt"></i>
                </div>
                <h3 class="card-title">{{this.title}}</h3>
                <p>{{this.description}}</p>
                <a href="{{this.link}}">{{this.linkText}}</a>
                <a href="#" class="ms-4">Connect</a>                
              </div>

            </div>

          </div>
          {{/each}}
        </div>

      </div>
    </section>

    {{!-- Section-2 --}}
    <div class="container py-5 ">
      <div class="row">
        <div class="col-md-6">
          <img src="static/images/img0.jpg" style="height: 250px;" class="img-fluid" alt="" srcset="">
